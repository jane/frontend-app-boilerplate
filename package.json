{
  "name": "frontend-boilerplate",
  "description": "Jane React/Koa boilerplate",
  "version": "0.0.1",
  "main": "server/index.js",
  "scripts": {
    "build": "node scripts/build.js",
    "format": "prettier \"{src,server,scripts,config}/**/*.js\" --write",
    "prebuild": "npm t",
    "precommit": "lint-staged",
    "start": "run-p start:server start:client",
    "start:client": "node scripts/start.js",
    "start:server": "nodemon --watch server server",
    "test": "run-s test:lint",
    "test:jest": "node scripts/test.js --env=jsdom",
    "test:lint": "eslint -c .eslintrc.json server/ src/ scripts/ config/"
  },
  "homepage": "https://github.com/jane/frontend-boilerplate#readme",
  "bugs": "https://github.com/jane/frontend-boilerplate/issues",
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-latest-minimal": "1.1.2",
    "babel-runtime": "6.26.0",
    "boring-cluster": "3.0.0",
    "final-form": "4.6.1",
    "fs-extra": "6.0.0",
    "jest": "22.4.3",
    "koa": "2.5.1",
    "koa-body-parser": "1.1.2",
    "koa-cors": "0.0.16",
    "koa-router": "7.4.0",
    "koa-simple-static": "1.1.0",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "qs": "6.5.2",
    "r2": "2.0.1",
    "raf": "3.4.0",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-final-form": "3.4.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "recompose": "0.27.0",
    "resolve": "1.7.1",
    "styled-components": "3.2.6",
    "whatwg-fetch": "2.0.4",
    "zeelib": "7.0.2"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.4",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-styled-name": "2.0.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-plugin-transform-regenerator": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-jane": "2.0.3",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jest": "21.15.1",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-unicorn": "4.0.3",
    "husky": "0.14.3",
    "lint-staged": "7.1.0",
    "nodemon": "1.17.4",
    "npm-run-all": "4.1.3",
    "prettier": "1.12.1",
    "prop-types": "15.6.1",
    "react-dev-utils": "5.0.1",
    "webpack": "4.8.1",
    "webpack-dev-server": "3.1.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js",
      "<rootDir>/src/**/?(*.)(spec|test).js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|json)$)": "<rootDir>/config/jest/file-transform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.js$"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "private": true,
  "proxy": "http://localhost:8081"
}
