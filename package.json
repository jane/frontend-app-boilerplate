{
  "name": "frontend-boilerplate",
  "description": "Jane React/Koa boilerplate",
  "version": "0.0.1",
  "license": "MIT",
  "main": "server/index.js",
  "scripts": {
    "build": "node scripts/build.js",
    "format": "fixpack && prettier \"{src,server,scripts,config}/**/*.js\" --write",
    "prebuild": "npm t",
    "precommit": "lint-staged",
    "start": "run-p start:server start:client",
    "start:client": "node scripts/start.js",
    "start:server": "nodemon --watch server server",
    "test": "run-s test:lint",
    "test:jest": "node scripts/test.js --env=jsdom",
    "test:lint": "eslint -c .eslintrc.json server/ src/ scripts/ config/"
  },
  "homepage": "https://github.com/jane/frontend-boilerplate#readme",
  "bugs": "https://github.com/jane/frontend-boilerplate/issues",
  "dependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-preset-latest-minimal": "1.1.2",
    "boring-cluster": "4.0.0",
    "final-form": "4.10.0",
    "fs-extra": "7.0.0",
    "jane-components": "0.1.2",
    "jest": "23.6.0",
    "koa": "2.5.3",
    "koa-body-parser": "1.1.2",
    "koa-cors": "0.0.16",
    "koa-lowercase": "1.2.2",
    "koa-router": "7.4.0",
    "koa-simple-static": "1.1.2",
    "object-assign": "4.1.1",
    "promise": "8.0.2",
    "qs": "6.5.2",
    "r2": "2.0.1",
    "raf": "3.4.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-final-form": "3.6.6",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-path-normalize": "1.3.2",
    "recompose": "0.30.0",
    "resolve": "1.8.1",
    "styled-components": "3.4.9",
    "styled-reset": "1.5.1",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-transform-async-to-generator": "7.1.0",
    "@babel/plugin-transform-regenerator": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-styled-components": "1.8.0",
    "babel-plugin-styled-name": "2.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.18",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "eslint": "5.6.1",
    "eslint-plugin-jane": "1.0.1",
    "fixpack": "2.3.1",
    "husky": "1.1.1",
    "lint-staged": "7.3.0",
    "nodemon": "1.18.4",
    "npm-run-all": "4.1.3",
    "prettier": "1.14.3",
    "prop-types": "15.6.2",
    "react-dev-utils": "6.0.4",
    "webpack": "4.20.2",
    "webpack-dev-server": "3.1.9"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js",
      "<rootDir>/src/**/?(*.)(spec|test).js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|json)$)": "<rootDir>/config/jest/file-transform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.js$"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "private": true,
  "proxy": "http://localhost:8081"
}
