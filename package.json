{
  "name": "frontend-boilerplate",
  "description": "Jane React/Koa boilerplate",
  "version": "0.0.4",
  "private": true,
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "run-s build:server build:client",
    "build:client": "node scripts/build.js",
    "build:server": "tsc src/server/*.ts --outDir lib/server 2>&1 >/dev/null || true",
    "clean": "rm -rf lib && mkdir -p lib/{server,client}",
    "format": "prettier \"{src,server,scripts,config}/**/*.{js,ts,tsx}\" --write",
    "prebuild": "npm run clean && sortpack && npm t",
    "preversion": "npm run build",
    "start": "run-p start:server start:client",
    "start:client": "node scripts/start.js",
    "start:server": "nodemon",
    "test": "run-s test:lint",
    "test:jest": "node scripts/test.js --env=jsdom",
    "test:lint": "eslint -c .eslintrc.json --ext .ts,.tsx,.js src/ scripts/ config/"
  },
  "homepage": "https://github.com/jane/frontend-boilerplate#readme",
  "bugs": "https://github.com/jane/frontend-boilerplate/issues",
  "dependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-proposal-object-rest-spread": "7.7.7",
    "@babel/polyfill": "7.7.0",
    "@babel/register": "7.7.7",
    "@babel/runtime": "7.7.7",
    "@koa/router": "8.0.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-preset-latest-minimal": "1.1.2",
    "boring-cluster": "6.0.3",
    "fetchyeah": "3.0.6",
    "final-form": "4.18.6",
    "fs-extra": "8.1.0",
    "jane-components": "1.0.0",
    "jest": "24.9.0",
    "koa": "2.11.0",
    "koa-bodyparser": "4.2.1",
    "koa-cors": "0.0.16",
    "koa-lowercase": "2.0.4",
    "koa-simple-static": "3.0.3",
    "object-assign": "4.1.1",
    "promise": "8.0.3",
    "qs": "6.9.1",
    "raf": "3.4.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-final-form": "6.3.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-router-path-normalize": "3.0.6",
    "recompose": "0.30.0",
    "resolve": "1.14.1",
    "styled-components": "4.4.1",
    "styled-reset": "4.0.8",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-transform-async-to-generator": "7.7.4",
    "@babel/plugin-transform-regenerator": "7.7.5",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "7.7.4",
    "@babel/preset-typescript": "7.7.7",
    "@types/koa": "2.11.0",
    "@types/koa-bodyparser": "4.3.0",
    "@types/koa-cors": "0.0.0",
    "@types/koa__router": "8.0.2",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/react-router": "5.1.3",
    "@types/react-router-dom": "5.1.3",
    "@types/styled-components": "4.4.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.6",
    "babel-plugin-styled-name": "2.0.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-plugin-jane": "7.0.1",
    "http-proxy-middleware": "0.20.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "nodemon": "2.0.2",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "react-dev-utils": "10.0.0",
    "sortpack": "2.0.4",
    "ts-node": "8.5.4",
    "typescript": "3.7.4",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack": "4.41.5",
    "webpack-dev-server": "3.10.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js",
      "<rootDir>/src/**/?(*.)(spec|test).js"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|json)$)": "<rootDir>/config/jest/file-transform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.js$"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "proxy": "http://localhost:8081",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
