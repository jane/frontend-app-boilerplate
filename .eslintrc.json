{
  "root": true,
  "extends": [
    "plugin:jane/recommended",
    "plugin:jane/typescript",
    "plugin:jane/prettier-ts"
  ],
  "plugins": [
    "jane"
  ],
  "rules": {
    "node/no-unpublished-require": 0,
    "no-console": 0,
    "@typescript-eslint/no-var-requires": 0
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
